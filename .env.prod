# FitTracker 生产环境配置
# 虚拟机测试环境配置

# 数据库配置
POSTGRES_DB=fittracker
POSTGRES_USER=fittracker
POSTGRES_PASSWORD=FitTracker2024!SecureDB
DB_SSL_MODE=disable

# Redis 配置
REDIS_PASSWORD=FitTracker2024!Redis

# JWT 配置
JWT_SECRET=FitTracker2024!JWTSecretKeyForTesting123456789
JWT_EXPIRE_HOURS=24

# AI 服务配置
DEEPSEEK_API_KEY=sk-test-key-for-development
DEEPSEEK_API_URL=https://api.deepseek.com/v1/chat/completions
TENCENT_HUNYUAN_API_KEY=test-key-for-development
TENCENT_HUNYUAN_API_URL=https://hunyuan.tencentcloudapi.com

# Docker 镜像配置
REGISTRY=localhost
NAMESPACE=fittracker
TAG=latest

# Grafana 配置
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=FitTracker2024!Grafana

# 域名配置（虚拟机测试）
DOMAIN=localhost
SSL_EMAIL=test@example.com

# 备份配置
BACKUP_RETENTION_DAYS=7
BACKUP_SCHEDULE=0 2 * * *

# 监控配置
ENABLE_MONITORING=true
ENABLE_LOGGING=true
LOG_LEVEL=info

# 数据库连接字符串
DATABASE_URL=postgres://fittracker:FitTracker2024!SecureDB@db:5432/fittracker?sslmode=disable

# Redis 连接字符串
REDIS_URL=redis://:FitTracker2024!Redis@redis:6379/0
